#LA 2
class Hero:
    def __init__(self, name, hp):
        self.name = name
        self.hp = hp
        self.basic_attack = 10
        self.power_type = "one tapper"
        self.role = "lurker"
        
lurker = Hero("Sharlotte",100)
print(lurker.name)
print(lurker.hp)
print(lurker.basic_attack)
print(lurker.power_type)
print(lurker.role)
